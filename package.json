{
  "name": "eslint-starter",
  "version": "1.0.0",
  "description": "Tiny eslint example",
  "main": "src/server.js",
  "scripts": {
    "start": "node .",
    "lint": "eslint src",
    "test": "npm run lint && echo \"Write some tests!\"",
    "watch": "nodemon --watch src --exec \"npm run test && npm run start\""
  },
  "keywords": [
    "eslint",
    "lint",
    "starter",
    "example",
    "atom",
    "standard"
  ],
  "author": "Michael Trotter",
  "license": "MIT",
  "repository": "https://github.com/spicydonuts/eslint-starter",
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^3.1.9"
  },
  "dependencies": {
    "express": "^4.21.2"
  }
}
